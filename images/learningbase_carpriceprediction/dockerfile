FROM busybox

# Create required directories
RUN mkdir -p /tmp/learningBase/train \
             /tmp/learningBase/test

# Copy datasets
COPY tmp/learningBase/train/training_data.csv /tmp/learningBase/train/
COPY tmp/learningBase/test/test_data.csv /tmp/learningBase/test/

# Copy documentation
COPY Readme.md /tmp/learningBase/Readme.md
